{
  "name": "innleveringwebogapi",
  "version": "1.0.0",
  "main": "hello.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently npm:dev:client npm:dev:server",
    "dev:client": "cd client && npx parcel watch index.html",
    "dev:server": "cd server && npx nodemon server.js",
    "postinstall": "npm run install:client && npm run install:server",
    "install:client": "cd client && npm install --include=dev",
    "install:server": "cd server && npm install",
    "build": "npm run check && cd client && npx parcel build index.html",
    "start": "cd server && node server.js",
    "check": "prettier --check .",
    "prettier": "prettier --write ."
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  }
}
